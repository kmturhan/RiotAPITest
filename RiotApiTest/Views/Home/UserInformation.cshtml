        <div class="item"><label>USER Name</label><br />@Model.UI.name<br /></div>
        <div class="item"><label>USER Level</label><br />@Model.UI.summonerLevel<br /></div>
        <div class="item"><label>USER ID</label><br />@Model.UI.id<br /></div>
        <div class="item"><label style="color:red;">AccountID</label><br />@Model.UI.accountId<br /></div>
        <div class="item"><label>PUUID</label><br />@Model.UI.puuid<br /></div>
        <div class="item"><label>Revision Date</label><br />@dt.ToLongDateString() @dt.ToLongTimeString() <br /></div>
        <div class="item"><label>Total Games</label><br />@Model.UM.totalGames<br /></div>
    </div>

    <div style="width:50%;display:flex;flex-wrap:wrap;justify-content:space-evenly;margin-top:20px;">

        @{int index = 1; }

        @foreach (var item in Model.UM.matches)
        {
            <div style="width:30%;border:1px solid black;text-align:left;padding-left:10px;margin-bottom:5px;">
                <label>HelpIndex : </label>@(index++)<br />
                <label>Lane : </label>@item.lane<br />
                <label>Champion : </label>@item.ChampionName<br />
                <label>GameID : </label>@item.gameId<br />
                <label>PlatformID : </label>@item.platformId<br />
                <label>Queue : </label>@item.queue<br />
                <label>Role : </label>@item.role<br />
                <label>Season : </label>@item.season<br />
                <label>TimeStamp : </label>@item.timestamp<br />
                <label><a href="@Url.Action("MatchInfo","Home",new {MatchID=item.gameId })">MatchInfo</a></label>
                <label><a href="@Url.Action("MatchTimeline","Home",new {MatchID=item.gameId })">MatchTimeline</a></label>
            
